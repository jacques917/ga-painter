<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane prefHeight="390.0" prefWidth="479.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8.0.121"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.github.jacques917.ga.painter.controller.ImagesPanelController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="323.0" minHeight="260.0" prefHeight="323.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="130.0" minHeight="10.0" prefHeight="67.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Original image" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
            <font>
                <Font name="Comic Sans MS" size="12.0"/>
            </font>
        </Label>
        <Label text="Current leader" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1"
               GridPane.valignment="CENTER">
            <font>
                <Font name="Comic Sans MS" size="12.0"/>
            </font>
        </Label>
        <ImageView fx:id="sourceImage" fitHeight="270.0" fitWidth="210.0" pickOnBounds="true" preserveRatio="true"
                   GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
        <ImageView fx:id="currentLeader" fitHeight="270.0" fitWidth="210.0" pickOnBounds="true" preserveRatio="true"
                   GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    </children>
</GridPane>
